{
  "name": "bastelfreak-roles",
  "version": "0.1.0",
  "summary": "a list of profiles for a complete puppet demo/lab environment",
  "author": "Tim Meusel",
  "dependencies": [
  ],
  "source": "https://github.com/bastelfreak/osmc2019.git",
  "project_page": "https://github.com/bastelfreak/osmc2019",
  "issues_url": "https://github.com/bastelfeak/osmc2019/issues",
  "license": "Apache-2.0",
  "tags": [
    "control repo",
    "controlrepo",
    "consul",
    "prometheus",
    "ferm",
    "ipset",
    "ipsets"
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.5.8 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "18.04"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10"
      ]
    },
    {
      "operatingsystem": "Archlinux"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 6.1.0 < 7.0.0"
    },
    {
      "name": "KyleAnderson/consul",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    },
    {
      "name": "puppet/prometheus",
      "version_requirement": ">= 7.1.0 < 8.0.0"
    },
    {
      "name": "puppet/nginx",
      "version_requirement": ">= 1.0.0 < 2.0.0"
    },
    {
      "name": "puppet/ferm",
      "version_requirement": ">= 2.7.0 < 3.0.0"
    },
    {
      "name": "puppet/ipset",
      "version_requirement": ">= 1.1.0 < 2.0.0"
    },
    {
      "name": "theforeman/puppet",
      "version_requirement": ">= 12.1.0 < 13.0.0"
    },
    {
      "name": "puppet/selinux",
      "version_requirement": ">= 3.0.0 < 4.0.0"
    }
  ]
}
